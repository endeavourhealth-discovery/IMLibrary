{
  "name": "im-library",
  "version": "1.0.0",
  "main": "dist/im-library.js",
  "type": "module",
  "scripts": {
    "clean": "rimraf dist && rimraf build",
    "build:tsc": "vue-tsc",
    "build:dev": "npm-run-all clean build:tsc build:all build:api build:finalize",
    "build:all": "cross-env LIB_NAME=all vite build",
    "build:api": "cross-env LIB_NAME=api vite build",
    "build:finalize": "shx mkdir ./build && shx cp -R ./dist ./build/dist && shx cp ./package.json ./build",
    "build:production": "npm run build:dev && npm pack",
    "docs:dev": "vitepress dev docs",
    "docs:build": "vitepress build docs",
    "docs:serve": "vitepress serve docs",
    "test:unit": "vitest",
    "test:coverage": "vitest run --coverage",
    "lint": "eslint --fix",
    "format": "prettier --write"
  },
  "peerDependencies": {
    "primeflex": "3.2.1",
    "primeicons": "5.0.0",
    "primevue": "3.15.0",
    "vue": "3.2.37",
    "vue-router": "4.0.16",
    "vuex": "4.0.2"
  },
  "devDependencies": {
    "@faker-js/faker": "^7.5.0",
    "@mswjs/data": "0.10.2",
    "@originjs/vite-plugin-commonjs": "1.0.3",
    "@testing-library/vue": "6.6.1",
    "@types/lodash": "4.14.182",
    "@types/node": "18.0.0",
    "@types/uuid": "8.3.4",
    "@vitejs/plugin-vue": "2.3.3",
    "@vue/test-utils": "2.0.0",
    "c8": "7.11.3",
    "cross-env": "7.0.3",
    "google-palette": "1.1.0",
    "jsdom": "20.0.0",
    "rimraf": "3.0.2",
    "sass": "1.52.3",
    "typescript": "4.7.4",
    "vite": "2.9.13",
    "vitepress": "1.0.0-alpha.2",
    "vitest": "0.15.1",
    "vue-tsc": "0.38.1"
  },
  "files": [
    "dist"
  ],
  "types": "dist/types/index.d.ts",
  "module": "./dist/im-library.js",
  "exports": {
    ".": "./dist/im-library.js",
    "./dist/api": "./dist/api.js",
    "./dist/style.css": "./dist/style.css"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "6.1.1",
    "@fortawesome/free-brands-svg-icons": "6.1.1",
    "@fortawesome/free-regular-svg-icons": "6.1.1",
    "@fortawesome/free-solid-svg-icons": "6.1.1",
    "@types/jsonpath": "0.2.0",
    "axios": "0.27.2",
    "headers-polyfill": "3.0.10",
    "jsonpath": "1.1.1",
    "lodash": "4.17.21",
    "msw": "0.47.2",
    "npm-run-all": "4.1.5",
    "semver": "^7.3.7",
    "shx": "0.3.4",
    "url": "0.11.0",
    "uuid-random": "1.3.2",
    "vuedraggable": "2.24.3"
  }
}
